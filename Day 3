int main()
{
    string N_temp;
    getline(cin, N_temp);

    int N = stoi(ltrim(rtrim(N_temp)));
    //int n=N_temp%2;
    if(N%2==0){
      
        if(N>=6 || N<=20 ){
          cout<<"Not Weird";
        }else {
          cout<<"Weird";
        }
        }else{
          cout<<"Weird";
        }
         return 0;
}
